{
  "name": "virtual-tryon-backend",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "prebuild": "npm install",
    "build": "tsc",
    "postbuild": "tsc-alias",
    "start": "node --experimental-specifier-resolution=node dist/server.js",
    "dev": "nodemon --exec ts-node --esm src/server.ts",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "@gradio/client": "^2.3.1",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "multer": "^1.4.5-lts.1",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.11.19"
  },
  "devDependencies": {
    "nodemon": "^3.0.3",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "tsc-alias": "^1.8.8"
  }
}